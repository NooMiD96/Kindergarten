{
  "name": "kindergarten",
  "version": "0.0.1",
  "description": "Kindergarten",
  "scripts": {
    "clean": "IF EXIST %CD%\\public\\client rd /s /q %CD%\\public\\client && IF EXIST %CD%\\public\\server rd /s /q %CD%\\public\\server",
    "buildDev": "npm run clean && webpack --config webpack.config.js --mode development",
    "buildProd": "npm run clean && webpack --config webpack.config.js --mode production --env.prod",
    "getBundlsStats": "npm run clean && webpack --mode production --env.prod --env.show --profile --json > stats.json"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "connected-react-router": "^6.3.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "antd": "^3.16.2",
    "@ant-design/icons": "^1.2.1",
    "styled-components": "^4.2.0",
    "domain-task": "^3.0.3"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-hot-loader": "^4.1.0",
    "@types/react-redux": "7.0.6",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack": "^4.4.27",
    "@types/webpack-env": "^1.13.9",
    "@types/styled-components": "^4.1.13",
    "aspnet-prerendering": "^3.0.1",
    "aspnet-webpack": "^3.0.0",
    "aspnet-webpack-react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-noop": "^1.0.0",
    "react-hot-loader": "^4.8.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-bundle-analyzer": "^3.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "webpack-manifest-plugin": "^2.0.4",
    "copy-webpack-plugin": "^5.0.2"
  }
}
